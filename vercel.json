{
  "rewrites": [
    {
      "source": "/reset-password",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!reset-password$).+)",
      "destination": "/",
      "permanent": false
    }
  ]
}

